{
  "name": "wetube",
  "version": "1.0.0",
  "description": "Cloning Youtube with Vanila and NodeJS",
  "main": "index.js",
  "author": "Jeongho Heo",
  "dependencies": {
    "@babel/core": "^7.4.4",
    "@babel/node": "^7.2.2",
    "@babel/polyfill": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "autoprefixer": "^9.5.1",
    "aws-sdk": "^2.442.0",
    "axios": "^0.18.0",
    "babel-loader": "^8.0.0-beta.6",
    "body-parser": "^1.18.3",
    "connect-mongo": "^2.0.3",
    "cookie-parser": "^1.4.4",
    "css-loader": "^2.1.1",
    "dotenv": "^7.0.0",
    "express": "^4.16.4",
    "express-session": "^1.16.1",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "get-blob-duration": "^1.0.1",
    "helmet": "^3.16.0",
    "mongoose": "^5.5.1",
    "morgan": "^1.9.1",
    "multer": "^1.4.1",
    "multer-s3": "^2.9.0",
    "node-sass": "^4.11.0",
    "passport": "^0.4.0",
    "passport-github": "^1.1.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-local": "^1.0.0",
    "passport-local-mongoose": "^5.0.1",
    "postcss-loader": "^3.0.0",
    "pug": "^2.0.3",
    "sass-loader": "^7.1.0"
  },
  "scripts": {
    "dev:server": "nodemon --delay 2 --exec babel-node src/init.js --ignore `scss`",
    "dev:assets": "cd src && webpack --mode=development -w",
    "build:assets": "cd src && webpack --mode=production",
    "build:server": "babel src --out-dir build --ignore 'src/static','src/assets','src/webpack.config.js'",
    "copyAll": "cp -R src/static build && cp -R src/views build",
    "build": "npm run build:server && npm run build:assets && npm run copyAll",
    "prebuild": "rm -rf build",
    "tunnel": "lt --port 4000",
    "start": "node build/init.js",
    "prestart": "npm run build"
  },
  "devDependencies": {
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-import": "^2.17.1",
    "nodemon": "^1.18.11",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.3.0"
  }
}
